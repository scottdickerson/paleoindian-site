{
    "extends": [
        "prettier",
        "plugin:@next/next/recommended",
        "eslint:recommended"
    ],

    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "error"
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    }
}
